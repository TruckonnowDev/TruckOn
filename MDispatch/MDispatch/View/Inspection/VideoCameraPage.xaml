<?xml version="1.0" encoding="utf-8" ?>
<video:VideoCameraPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MDispatch.View.Inspection.VideoCameraPage"
             xmlns:video="clr-namespace:MDispatch.NewElement.CustomVideoCam"
             OnPhotoResult="VideoCameraPage_OnPhotoResult">
    <ContentPage.Content>
        <StackLayout>
            <Image Margin="5" Source="BackIconpage.png"  HorizontalOptions="Start" AbsoluteLayout.LayoutFlags="PositionProportional" AbsoluteLayout.LayoutBounds=".02, .02, 40, 40">
                <Image.GestureRecognizers>
                    <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"/>
                </Image.GestureRecognizers>
            </Image>
        </StackLayout>
    </ContentPage.Content>
</video:VideoCameraPage>