<?xml version="1.0" encoding="utf-8" ?>
<camera:CameraPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MDispatch.View.Inspection.PickedUp.CameraPageFolder.CameraPthotoInTrack"
             xmlns:camera="clr-namespace:MDispatch.NewElement"
             OnPhotoResult="CameraPage_OnPhotoResult">
    <ContentPage.Content>
        <StackLayout >
            <Label x:Name="titlePhoto" HorizontalTextAlignment="Center" TextColor="Black" BackgroundColor="Silver" Opacity="0.7"/>
            <Image x:Name="paternPhoto" HorizontalOptions="Center" VerticalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</camera:CameraPage>