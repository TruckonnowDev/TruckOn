<script>
    var curentSelectPattern;

    function LoadFile(fille, numDock) {
        let pref = fille.files[0].name.substr(fille.files[0].name.lastIndexOf(".") + 1);
        let nameLoadDocEl = document.getElementById("nameLoadDoc" + numDock);
        let nameNoLoadDocEl = document.getElementById("nameNoLoadDoc" + numDock);
        if (pref == "pdf" || pref == "jpg" || pref == "jepg") {
            nameNoLoadDocEl.hidden = "hidden";
            nameLoadDocEl.hidden = "";
            nameLoadDocEl.textContent = fille.files[0].name;
        }
        else {
            nameNoLoadDocEl.hidden = "";
            nameLoadDocEl.hidden = "hidden";
            fille.value = "";
        }
    }

    function SelectTrailer(value) {
        if (curentSelectPattern || value == "") {
            curentSelectPattern.style.display = "none";
        }
        let selectTypeDiv = document.getElementById(value);
        if (selectTypeDiv) {
            selectTypeDiv.style.display = "flex";
            curentSelectPattern = selectTypeDiv;
        }
    }
</script>


